<p id="notice"><%= notice %></p>

<div class="panel panel-default">
  <div class="panel-heading">Listing Admins</div>
  <table class="table">
    <thead>
      <tr>
        <th>Email</th>
        <th>Name</th>
        <th colspan="3"></th>
      </tr>
    </thead>

    <tbody>
      <% @admins.each do |admin| %>
        <tr>
          <td><%= admin.email %></td>
          <td><%= "#{admin.first_name} #{admin.last_name}".titleize %></td>
        </tr>
      <% end %>
    </tbody>
  </table>
</div>
